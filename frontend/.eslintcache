     localStorage.setItem('token', json.token);\r\n        SetLoggedIn(true);\r\n        SetDisplayedForm('');\r\n        SetUsername(json.username);\r\n      }).catch(err => console.log(err));\r\n\r\n  };\r\n\r\n  function handle_signup(e, data) {\r\n    e.preventDefault();\r\n    fetch('/api/users/', {\r\n      method: 'POST',\r\n      headers: {\r\n        'Content-Type': 'application/json'\r\n      },\r\n      body: JSON.stringify(data)\r\n    })\r\n      .then(res => res.json())\r\n      .then(json => {\r\n        localStorage.setItem('token', json.token);\r\n        SetLoggedIn(true);\r\n        SetDisplayedForm('');\r\n        SetUsername(json.username);\r\n      }).catch(err => console.log(err));;\r\n  };\r\n\r\n\r\n  function handle_logout() {\r\n    localStorage.removeItem('token');\r\n    SetLoggedIn(false);\r\n    SetUsername('')\r\n      .catch(err => console.log(err));\r\n  };\r\n\r\n\r\n  function display_form(form) {\r\n    SetDisplayedForm(form);\r\n\r\n  };\r\n\r\n\r\n  function loggedIn() {\r\n    return (\r\n      <React.Fragment>\r\n\r\n\r\n        <Router>\r\n          <Switch>\r\n            <Route exact path='/' component={HomePage} />\r\n            <Route exact path='/home' component={HomePage} />\r\n            <Route exact path='/lists' component={ToDoList} />\r\n            <Route exact path='/lists/:listID' component={Items} />\r\n            {/*<Route path='/login' component={Login} handle_login={handle_login} />\r\n            <Route path='/register' component={Register} />*/}\r\n          </Switch>\r\n        </Router>\r\n      </React.Fragment>\r\n\r\n    )\r\n  }\r\n\r\n  function loggedOut() {\r\n    return (\r\n      <React.Fragment>\r\n        <Container style={{ marginTop: '50px' }}>\r\n          <Login handle_login={handle_login}> </Login>\r\n          <div className=\"home-card\">\r\n            <Card>\r\n              <Card.Body>\r\n                <Card.Header>Login or Signup Now !</Card.Header>\r\n                <Card.Subtitle className=\"mb-2 mt-2 text-muted\">Each user has User-specific data</Card.Subtitle>\r\n                <Card.Text>\r\n                  Signup to start getting things done !\r\n                                </Card.Text>\r\n              </Card.Body>\r\n            </Card>\r\n          </div>\r\n          <Register handle_signup={handle_signup}></Register>\r\n        </Container>\r\n      </React.Fragment>\r\n    )\r\n  }\r\n\r\n\r\n  return (\r\n    <React.Fragment>\r\n      <Navbar1\r\n        logged_in={logged_in}\r\n        display_form={display_form}\r\n        handle_logout={handle_logout}\r\n        username={username}\r\n      />\r\n      <div>\r\n        {logged_in ? loggedIn() : loggedOut()}\r\n      </div>\r\n      <Footer></Footer>\r\n    </React.Fragment>\r\n\r\n  )\r\n}\r\n","E:\\Programming\\Web Projects\\React-Django-ToDoList-2.0\\frontend\\src\\components\\footer.jsx",[],"E:\\Programming\\Web Projects\\React-Django-ToDoList-2.0\\frontend\\src\\components\\homepage.jsx",[],"E:\\Programming\\Web Projects\\React-Django-ToDoList-2.0\\frontend\\src\\components\\items.jsx",["119","120","121","122","123","124"],"import { useState, useEffect } from 'react'\r\nimport axios from \"axios\";\r\nimport NewItem from \"./newitem\";\r\nimport { InputGroup, FormControl } from \"react-bootstrap\";\r\nimport Table from 'react-bootstrap/Table'\r\nimport Container from 'react-bootstrap/Container'\r\nimport Button from 'react-bootstrap/Button'\r\nimport { Link } from 'react-router-dom';\r\nimport { Dropdown, ButtonGroup, DropdownButton, SplitButton } from 'react-bootstrap';\r\n\r\n\r\n\r\nfunction Items(props) {\r\n    const listID = props.match.params.listID;\r\n    const name = props.location.nameList;\r\n    const [allItems, SetAllItems] = useState([{\r\n        id: \"\",\r\n        text: \"\",\r\n        complete: \"\",\r\n    }]);\r\n    const [newItem, SetNewItem] = useState([{\r\n        id: \"\",\r\n        text: \"\",\r\n        complete: false,\r\n    }])\r\n    useEffect(() => {\r\n        getAllItems();\r\n    }, [deleteItem, updateItem, handleSubmitButton]);\r\n\r\n    function getAllItems() {\r\n        axios.get(`/api/get-items/${listID}`,\r\n            { headers: { \"authorization\": `JWT ${localStorage.getItem('token')}` } })\r\n            .then(response => {\r\n                SetAllItems(response.data);\r\n\r\n            })\r\n            .catch(err => console.log(err));\r\n    }\r\n\r\n\r\n\r\n    function deleteItem(item) {\r\n        const index = allItems.indexOf(item);\r\n        axios.delete(`/api/delete-item/${allItems[index].id}`,\r\n            { headers: { \"authorization\": `JWT ${localStorage.getItem('token')}` } })\r\n            .catch(err => console.log(err));\r\n    }\r\n\r\n    function updateItem(item) {\r\n        const index = allItems.indexOf(item);\r\n        const requestOptions = {\r\n            method: \"PUT\",\r\n            headers: {\r\n                \"Content-Type\": \"application/json\",\r\n                \"authorization\": `JWT ${localStorage.getItem('token')}`\r\n            },\r\n        };\r\n        axios.put(`/api/update-item/${allItems[index].id}`, {\r\n            text: allItems[index].text,\r\n            complete: allItems[index].complete,\r\n            name: listID\r\n        }, requestOptions).catch(err => console.log(err));\r\n    }\r\n\r\n\r\n    function showStatus(item) {\r\n        const index = allItems.indexOf(item);\r\n        if (allItems[index].complete === true) {\r\n            return (<td>Completed</td>);\r\n        }\r\n        else {\r\n            return (<td>In-Complete</td>);\r\n        }\r\n    }\r\n\r\n\r\n    function handleUpdateText(item, e) {\r\n        const index = allItems.indexOf(item);\r\n        const array = [...allItems];\r\n        array[index].text = e.target.value;\r\n        SetAllItems(array);\r\n        console.log(allItems[index].text)\r\n\r\n    }\r\n\r\n\r\n    function handleUpdateComplete(item, e) {\r\n        const index = allItems.indexOf(item);\r\n        const array = [...allItems];\r\n        array[index].complete = e.target.checked;\r\n        SetAllItems(array);\r\n    }\r\n\r\n    function renderDropdown() {\r\n        return (\r\n            <DropdownButton\r\n                id=\"dropdown-basic-button\"\r\n                title=\"Show Items\"\r\n                variant='dark'>\r\n                <Dropdown.Item eventKey='1' onSelect={(e) => showItems(e)}>All Items</Dropdown.Item>\r\n                <Dropdown.Item eventKey='2' onSelect={(e) => showItems(e)}>Completed Items</Dropdown.Item>\r\n                <Dropdown.Item eventKey='3' onSelect={(e) => showItems(e)}>In-Completed Items</Dropdown.Item>\r\n            </DropdownButton >\r\n        )\r\n    }\r\n\r\n    function showItems(e) {\r\n        if (e === '1') {\r\n\r\n        }\r\n        if (e === '2') {\r\n            const array = [allItems.filter(item => item.complete == true)];\r\n            SetAllItems(array);\r\n        }\r\n        if (e === '3') {\r\n            const array = [allItems.filter(item => item.complete == false)];\r\n            SetAllItems(array);\r\n        }\r\n    }\r\n\r\n    //Adding a new item\r\n    function handleAddText(e) {\r\n        const array = [...newItem];\r\n        array.text = e.target.value;\r\n        SetNewItem(array);\r\n    }\r\n\r\n    function handleSubmitButton() {\r\n        const requestOptions = {\r\n            method: \"POST\",\r\n            headers: {\r\n                \"Content-Type\": \"application/json\",\r\n                \"authorization\": `JWT ${localStorage.getItem('token')}`\r\n            },\r\n\r\n        };\r\n        console.log(listID)\r\n        axios.post(\"/api/add-item/\", {\r\n            text: newItem.text,\r\n            complete: newItem.complete,\r\n            name: listID,\r\n        }, requestOptions)\r\n        console.log(newItem);\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <Container style={{ marginTop: '100px' }}>\r\n            <h3>Items in: {name}</h3>\r\n            {renderDropdown()}\r\n            <Table striped bordered hover className='mt-3'>\r\n\r\n                <thead>\r\n                    <tr>\r\n                        <th>ID</th>\r\n                        <th>Status</th>\r\n                        <th>Item Name</th>\r\n                        <th>Edit</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {allItems.map((item, index) => (\r\n                        <tr key={item.id}>\r\n                            <td>{index + 1}</td>\r\n                            {showStatus(item)}\r\n                            <td>\r\n                                <InputGroup className=\"mb-1 mt-1\">\r\n                                    <InputGroup.Prepend>\r\n                                        <InputGroup.Checkbox\r\n                                            checked={item.complete}\r\n                                            onChange={(e) => handleUpdateComplete(item, e, index)} />\r\n                                    </InputGroup.Prepend>\r\n                                    <FormControl\r\n                                        aria-label=\"Text input with checkbox\"\r\n                                        placeholder={item.text}\r\n                                        onChange={(e) => handleUpdateText(item, e)} />\r\n                                </InputGroup>\r\n                            </td>\r\n                            <td><Button\r\n                                className=\"btn btn-sm mr-2\"\r\n                                onClick={() => updateItem(item)}>Update\r\n                                        </Button>\r\n                                <Button\r\n                                    className=\"btn btn-sm btn-danger\"\r\n                                    onClick={() => deleteItem(item)}>Delete\r\n                                        </Button>\r\n                 